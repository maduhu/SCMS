  <data name="ReportDataSource.SelectCommand" xml:space="preserve">
    <value>SELECT PR.RefNumber,PR.MBValue,PR.PaymentRqstType,PR.Remarks,PR.RequestDate,PR.SupplierId,SUP.Name AS SupplierName,
case when PR.RequestFor='FullPayment' then CONVERT(bit,1) else CONVERT(BIT,0) End as FullPayment,case when PR.RequestFor='Rate/Installment' then CONVERT(bit,1)else convert(bit,0) END AS RateInstallment 
,Case when PR.RequestFor='Adv.Payment' then CONVERT(bit,1) else convert(bit,0) End as AdvPayment,
 case when PR.RequestFor='Adv.FinalPayment' then CONVERT(bit,1) Else CONVERT(bit,0) End as AdvFinalPayment,
PR.Subject,PR.TotalAmount,CUR.ShortName as MasterBudgetCurrency,PT.Description as PaymentTerm,
PTY.Description as PaymentType,PO.Id as PurchaseOrderId,PO.RefNumber as PoNo,PB.LineNumber,PP.ProjectNumber,PB.Description,PBL.Amount,
PR.PreparedOn, PREP.StaffName AS PreparedByName,PREP.Designation as PreparedByTitle,PREP.SignatureImage as PreparedBySignature,
PR.ReviewedOn,COALESCE(REV.StaffName,'Not Reviewed') AS ReviewedByName,REV.Designation as ReviewedByTitle,REV.SignatureImage as ReviewedBySignature,
PR.AuthorizedOn,COALESCE(AUTH.StaffName,'Not Authorized') AS AuthorizedByName,AUTH.Designation AS AuthorizerTitle,AUTH.SignatureImage as AuthorizedBysignature fROM PaymentRequest PR INNER JOIN
PaymentRequestBudgetLine PBL ON PBL.PaymentRequestId=PR.Id INNER JOIN
Currency CUR ON CUR.Id = PR.CurrencyId INNER JOIN 
PaymentTerm PT ON PT.Id=PR.PaymentTermId INNER JOIN
PaymentType PTY ON PTY.Id = PR.PaymentTypeId INNER JOIN 
PurchaseOrder PO ON PO.Id = PR.PurchaseOrderId INNER JOIN 
ProjectBudget PB ON PB.Id= PBL.BudgetLineId INNER JOIN 
BudgetCategory BC ON BC.Id = PB.BudgetCategoryId INNER JOIN 
ProjectDonor PRO ON PRO.Id = BC.ProjectDonorId INNER JOIN 
Project  PP ON PP.Id = PRO.ProjectId INNER JOIN 
Supplier SUP ON SUP.Id = PR.SupplierId INNER JOIN
VStaffDetails PREP ON PREP.StaffID  = PR.PreparedBy INNER JOIN
VStaffDetails REV ON REV.StaffID = PR.ReviewedBy INNER JOIN
VStaffDetails AUTH ON AUTH.StaffID = PR.AuthorizedBy
</value>
