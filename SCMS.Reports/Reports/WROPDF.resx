  <data name="ReportDataSource.SelectCommand" xml:space="preserve">
    <value>select WRO.Id AS WareHouseReleaseId,WRO.RefNumber ,WRO.PreparedOn,WRO.RequestDate,WRO.ApprovedOn,WRO.CountryProgrammeId,WHO.Name AS WareHouseName,LOC.Name as WareHouseLocation,
WRO.GoodCondition,WRO.NotReleased,WRO.CorrectQuantity,REQ.StaffID AS RequestorId,REQ.StaffName as Requestor,
REQ.Designation as RequestorTitle,REQ.SignatureImage as RequestorSignature,REQ.SubOfficeName as RequesterOffice,
coalesce(convert(nvarchar(50),APP.StaffID),'N/A') as ApprovedId,COALESCE(APP.StaffName,'Not Approved')as ApprovedBy,
APP.Designation as ApproverTitle,APP.SignatureImage as ApproverSignature,coalesce(convert(nvarchar(50),RECP.StaffID),'N/A') as ReceivedById,
COALESCE(RECP.StaffName,'Not Received') as ReceivedBy,RECP.Designation as ReceivedByTitle,RECP.SignatureImage as ReceivedBySignature,
WRO.ReceiptDate,coalesce(convert(nvarchar(50),RELE.StaffID),'N/A') as ReleasedById,COALESCE(RELE.StaffName,'Not Released') as ReleasedBy,
RELE.Designation as ReleasedByTitle,RELE.SignatureImage as ReleasedBySignature,WRO.ReleaseDate,
WIT.Comments,WIT.Quantity,WIT.QuantityDamaged,WIT.QuantityReceived,
case when WIT.InventoryId&lt;&gt;'00000000-0000-0000-0000-000000000000' then T.Name when WIT.AssetId&lt;&gt;'00000000-0000-0000-0000-000000000000'
then AST.Name End as ItemDescription,
 case when WIT.AssetId&lt;&gt;'00000000-0000-0000-0000-000000000000' then AST.Weight End AS WEIGHT,
 CASE when WIT.InventoryId&lt;&gt;'00000000-0000-0000-0000-000000000000' then UNIT.Code 
  End as Unit
 from WarehouseRelease  WRO
INNER JOIN WarehouseReleaseItem WIT ON WRO.Id= WIT.WarehouseReleaseId
INNER JOIN WareHouse WHO ON WRO.WareHouseId = WHO.Id
INNER JOIN Location LOC ON WHO.LocationId = Loc.Id
INNER JOIN Inventory INV ON INV.Id=WIT.InventoryId
LEFT JOIN Asset AST ON AST.Id=WIT.AssetId 
INNER JOIN Item T ON T.Id=INV.ItemId
INNER JOIN UnitOfMeasure UNIT ON UNIT.Id=T.UnitOfMessureId
INNER JOIN VStaffDetails REQ ON REQ.StaffID=WRO.PreparedBy
INNER JOIN VStaffDetails APP ON APP.StaffID = WRO.ApprovedBy
INNER JOIN VStaffDetails RECP ON RECP.StaffID = WRO.ReceivedBy
LEFT JOIN VStaffDetails RELE ON RELE.StaffID = WRO.ReleasedBy




</value>
  </data>
