  <data name="CompletionCertificateDataSource.SelectCommand" xml:space="preserve">
    <value>select CC.Id AS CompletionCertificateId,CC.CountryProgrammeId,CC.RefNumber,CONVERT(VARCHAR(11), CC.PreparedOn, 106) as PreparedDate,CC.PreparedOn,CC.Purpose,CC.ProjectTitle,CC.Location,CC.WorksService,CC.Remarks,
CC.IsSubmitted,CC.IsApproved,CC.IsRejected,PO.RefNumber as PONumber,SUP.Name AS ContractorName,PREP.StaffID as PreparedById,PREP.StaffName as PreparedBy,PREP.Designation as PreparedByTitle,
PREP.SignatureImage as PreparedBySignature,PREP.SubOfficeName AS PreparedByOffice,COU.Name AS Country,APP.StaffID as ApprovedById, APP.StaffName AS ApprovedBy,APP.Designation AS ApprovedByTitle,
APP.SignatureImage AS ApprovedBySignature,APP.SubOfficeName as ApproveByOffice,CONF.StaffID as ConfirmedById,CONF.StaffName AS ConfirmedBy,CONF.Designation AS ConfirmedByTitle,
CONF.SignatureImage as ComfirmedBySignature,CONF.SubOfficeName as ConfirmedByOffice,DRC.Name as DRCOffice,
case when WorksService='Completed Satisfactorily' then CONVERT(BIT, 1) else CONVERT(BIT, 0) End as Satisfactorily,case when WorksService='Outstanding issues' then CONVERT(BIT, 1) Else CONVERT(BIT, 0) End as OutStanding, case when WorksService='Not Completed' then CONVERT(BIT, 1) else CONVERT(BIT, 0) End as Completed from CompletionCertificate CC
INNER JOIN PurchaseOrder PO ON PO.Id=CC.PurchaseOrderId
INNER JOIN Supplier SUP ON SUP.Id = PO.SupplierId
INNER JOIN CountrySubOffice OFC ON OFC.Id= CC.OfficeId
INNER JOIN CountrySubOffice DRC ON DRC.Id = CC.DRCOfficeId
INNER JOIN VStaffDetails PREP ON PREP.StaffID = CC.PreparedBy
INNER JOIN CountryProgramme CP ON PREP.CountryProgrammeId=CP.Id
INNER JOIN Country COU ON COU.Id = CP.CountryId
INNER JOIN VStaffDetails APP ON APP.StaffID = CC.ApprovedBy
INNER JOIN VStaffDetails CONF ON CONF.StaffID = CC.ConfirmedBy

</value>
